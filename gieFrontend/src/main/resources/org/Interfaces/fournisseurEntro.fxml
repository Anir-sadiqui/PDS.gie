<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="550.0" prefWidth="939.0" style="-fx-background-color: #808080;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.giefront.controller.FourniseurEntoContro">
    <children>
        <Pane fx:id="pane1" layoutX="4.0" layoutY="16.0" prefHeight="475.0" prefWidth="224.0" style="-fx-background-color: #ffffff;">
            <children>
                <ImageView fx:id="sup" fitHeight="65.0" fitWidth="67.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="https://cdn1.iconfinder.com/data/icons/global-business-4/64/headquarter-1024.png" />
                    </image>
                </ImageView>
                <Label layoutX="4.0" layoutY="82.0" prefHeight="16.0" prefWidth="35.0" text="Type">
                    <font>
                        <Font size="14.0" />
                    </font></Label>
                <Button fx:id="entroToPerso" layoutX="44.0" layoutY="66.0" mnemonicParsing="false" onAction="#switchToFornEntro" prefHeight="43.0" prefWidth="172.0" style="-fx-background-color: #000000;" text="EntroToPerson" textFill="WHITE" />
            </children>
        </Pane>
        <Pane fx:id="pane2" layoutX="235.0" layoutY="16.0" prefHeight="475.0" prefWidth="575.0" style="-fx-background-color: #ffffff;">
            <children>
                <TableView fx:id="fournisseurTable" layoutX="27.0" layoutY="75.0" prefHeight="265.0" prefWidth="527.0">
                    <columns>
                        <TableColumn fx:id="colId" prefWidth="75.0" text="Id" />
                        <TableColumn fx:id="colRaisonS" prefWidth="75.0" text="RaisonSocial" />
                        <TableColumn fx:id="colFormJ" prefWidth="75.0" text="FormeJuridique" />
                        <TableColumn fx:id="colPhone" prefWidth="75.0" text="Phone" />
                        <TableColumn fx:id="colEmail" prefWidth="75.0" text="Email" />
                        <TableColumn fx:id="colEdit" prefWidth="75.0" text="Edit" />
                        <TableColumn fx:id="colDelete" prefWidth="75.0" text="Delete" />
                    </columns>
                </TableView>
                <Button fx:id="retchbtn" layoutX="27.0" layoutY="25.0" mnemonicParsing="false" onAction="#fetchFournisseurs" prefHeight="37.0" prefWidth="65.0" text="Fetch" />
                <Button fx:id="addFournisseurPerso" layoutX="376.0" layoutY="25.0" mnemonicParsing="false" onAction="#addFournisseur" prefHeight="37.0" prefWidth="172.0" text="Add a new Fournisseur">
                    <graphic>
                        <ImageView fitHeight="24.0" fitWidth="22.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="https://www.downloadclipart.net/large/27475-add-button-clipart.png" />
                            </image>
                        </ImageView>
                    </graphic></Button>
            </children></Pane>
    </children>
</AnchorPane>
